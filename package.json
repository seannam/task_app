{
  "name": "rest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel src -d dist",
    "start": "npm run build && node dist/server/index.js",
    "start-dev": "nodemon src/server --exec babel-node",
    "docker:build": "docker-compose up --build",
    "docker:up": "docker-compose up",
    "lint": "semistandard --env=mocha",
    "format": "semistandard --env=mocha --fix",
    "test": "NODE_ENV=test mocha src/tests/**/*.test.js --require @babel/register --collect-coverage --exit",
    "test:ci": "NODE_ENV=test nyc mocha src/tests/**/*.test.js --require @babel/register --collect-coverage --exit --reporter mocha-junit-reporter --reporter-options mochaFile=reports/mocha/test-results.xml",
    "coverage": "nyc yarn test",
    "create-db": "sequelize-cli db:create",
    "drop-db": "sequelize-cli db:drop",
    "apply-migrations": "sequelize-cli db:migrate",
    "revert-migrations": "sequelize-cli db:migrate:undo:all",
    "apply-seeders": "sequelize-cli db:seed:all",
    "revert-seeders": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@admin-bro/express": "^3.1.0",
    "@admin-bro/sequelize": "^1.2.1",
    "admin-bro": "^3.2.5",
    "cookie-parser": "~1.4.4",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.1",
    "express-validation": "^3.0.6",
    "http-errors": "~1.8.0",
    "http-status": "^1.5.0",
    "jade": "^1.9.2",
    "mocha-junit-reporter": "^2.0.0",
    "morgan": "~1.10.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sqlite3": "^4.2.0",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.12.13",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "debug": "^4.3.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.1.0",
    "mocha": "~9.0.3",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "semistandard": "^16.0.1",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.1",
    "tsconfig-paths": "3.10.1"
  }
}
